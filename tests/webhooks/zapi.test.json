  "test_commands": {
    "curl_text_message": "curl -X POST http://localhost:3333/webhooks/zapi/message-received -H \"Content-Type: application/json\" -d '{\"waitingMessage\":false,\"isGroup\":false,\"instanceId\":\"your_instance_id\",\"messageId\":\"test123\",\"phone\":\"5511999887766\",\"fromMe\":false,\"momment\":1634567890123,\"chatName\":\"João Teste\",\"senderName\":\"João Teste\",\"type\":\"ReceivedCallback\",\"text\":{\"message\":\"Mensagem de teste\"}}'",
    "curl_status_update": "curl -X POST http://localhost:3333/webhooks/zapi/message-status -H \"Content-Type: application/json\" -d '{\"messageId\":\"test123\",\"instanceId\":\"your_instance_id\",\"phone\":\"5511999887766\",\"status\":\"delivered\",\"momment\":1634567890890}'",
    "curl_queue_status": "curl -X GET http://localhost:3333/webhooks/zapi/queue-status",
    "curl_health_check": "curl -X GET http://localhost:3333/health"
  },
  "validation_notes": {
    "required_fields": [
      "event",
      "instanceId",
      "data.messageId",
      "data.instanceId",
      "data.phone",
      "data.fromMe",
      "data.momment",
      "data.status",
      "data.type"
    ],
    "phone_format": "Apenas números, exemplo: 5511999887766",
    "momment_format": "Unix timestamp em millisegundos",
    "event_types": [
      "received-callback",
      "delivery-callback"
    ],
    "message_types": [
      "text",
      "image",
      "audio",
      "video",
      "document",
      "location",
      "contact",
      "sticker",
      "button_response",
      "list_response"
    ],
    "status_types": [
      "sent",
      "delivered",
      "read",
      "failed",
      "pending"
    ]
  }
